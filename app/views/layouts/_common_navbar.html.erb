<li><%= link_to "Home", root_path %></li>
<% if current_user %>
  <li class="twitter-name-correction">Welcome <%= current_user.nickname %></li>
  <li><%= image_tag(current_user.image_url, class: "img-circle img-thumbnail")%></li>
  <li><%= link_to "My Account", user_path %></li>
  <li class="nav-right "> <%= link_to "Logout", logout_path %></li>
<% else %>
  <li><%= link_to image_tag("sign-in-with-twitter-gray.png"), '/auth/twitter' %> </li>
<% end %>